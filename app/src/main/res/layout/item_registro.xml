            <TextView
                android:id="@+id/tv_idPCec"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text=""
                android:textSize="16sp" />
        </TableRow>

        <TableRow>
            <TextView
                android:id="@+id/tv_CalculoI"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/calcularInicio"
                android:textSize="16sp"
                android:paddingEnd="8dp" />

            <TextView
                android:id="@+id/tv_idCalculoI"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text=""
                android:textSize="16sp" />
        </TableRow>

        <TableRow>
            <TextView
                android:id="@+id/tv_ExamesRep"
